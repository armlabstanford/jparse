<launch>
    <!-- Declare parameters with default values -->
    <arg name="robot_ip" default="192.168.1.235" />

    <!-- param for use_native_xarm_spacemouse -->
    <arg name="use_native_xarm_spacemouse" default="false" />

    <node name="xarm_spacemouse" pkg="manipulator_control" type="xarm_spacemouse.py"  output="screen">
        <param name="robot_ip" value="$(arg robot_ip)" />
        <param name="use_native_xarm_spacemouse" value="$(arg use_native_xarm_spacemouse)" />
    </node>

</launch>